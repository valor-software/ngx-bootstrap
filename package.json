    "core-js": "2.5.1",
    "cpy": "5.1.0",
    "cpy-cli": "1.0.1",
    "cucumber": "3.1.0",
    "gh-pages": "1.1.0",
    "gitignore-to-glob": "0.3.0",
    "google-code-prettify": "1.0.5",
