{
  "name": "ng2-bootstrap",
  "version": "1.1.16-2",
  "description": "angular2 bootstrap components",
  "private": true,
  "scripts": {
    "flow.docs": "typedoc --exclude '**/*.spec.ts' ./src/",
    "flow.compile": "ngm",
    "flow.clean": "del-cli dist demo/dist temp coverage node_modules/ng2-bootstrap",
    "flow.deploy:gh-pages": "npm run build && ng build -p && gh-pages -d demo/dist",
    "flow.lint": "tslint \"**/*.ts\" -c tslint.json -e \"node_modules/**\" -e \"dist/**\" -e \"temp/**\"",
    "flow.changelog": "conventional-changelog -i CHANGELOG.md -s -p angular -v",
    "flow.github-release": "conventional-github-releaser -p angular",
    "preflow.publish": "run-s flow.clean flow.compile",
    "flow.publish": "echo 'here will be publish script' && exit 1",
    "postflow.publish": "npm run flow.deploy:gh-pages",
    "build": "run-s flow.clean flow.compile",
    "disable-prestart": "chokidar 'src/**/*.ts' -c \"npm run build\" --initial",
    "start": "ng serve",
    "build:copy": "cpy '**/*.*' ../node_modules/ng2-bootstrap --cwd=dist --parents",
    "pretest": "run-s flow.lint build:copy",
    "test": "ng test --watch false",
    "test:watch": "ng test",
    "preversion": "npm test",
    "version": "npm run flow.changelog && git add -A",
    "postversion": "git push origin development && git push --tags",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor"
  },
  "main": "bundles/ng2-bootstrap.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "angular2",
    "bootstrap",
    "angularjs",
    "twitter-bootstrap"
  ],
  "author": "Dmitriy Shekhovtsov <valorkin@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/valor-software/ng2-bootstrap.git"
  },
  "bugs": {
    "url": "https://github.com/valor-software/ng2-bootstrap/issues"
  },
  "homepage": "https://github.com/valor-software/ng2-bootstrap#readme",
  "dependencies": {
    "moment": "2.16.0"
  },
  "peerDependencies": {
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.0",
    "@angular/core": "^2.0.0",
    "@angular/forms": "^2.0.0"
  },
  "devDependencies": {
    "@angular/common": "2.2.0",
    "@angular/compiler": "2.2.0",
    "@angular/compiler-cli": "2.2.0",
    "@angular/core": "2.2.0",
    "@angular/forms": "2.2.0",
    "@angular/platform-browser": "2.2.0",
    "@angular/platform-browser-dynamic": "2.2.0",
    "@angular/platform-server": "2.2.0",
    "@angular/router": "3.2.0",
    "@angular/upgrade": "2.2.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "6.0.48",
    "@types/webpack": "2.0.0",
    "angular-cli": "1.0.0-beta.20-4",
    "bootstrap": "3.3.7",
    "chokidar-cli": "1.2.0",
    "codecov": "1.0.1",
    "codelyzer": "1.0.0-beta.4",
    "conventional-changelog-cli": "1.2.0",
    "conventional-github-releaser": "1.1.3",
    "core-js": "2.4.1",
    "cpy": "4.0.1",
    "cpy-cli": "1.0.1",
    "del-cli": "0.2.0",
    "gitignore-to-glob": "0.3.0",
    "google-code-prettify": "1.0.5",
    "html-loader": "0.4.4",
    "jasmine": "2.5.2",
    "jasmine-core": "2.5.2",
    "jasmine-data-provider": "2.2.0",
    "jasmine-spec-reporter": "2.7.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "lite-server": "2.2.2",
    "lodash": "4.17.2",
    "markdown-loader": "github:valorkin/markdown-loader",
    "marked": "0.3.6",
    "npm-run-all": "3.1.1",
    "npm-submodules": "0.1.4",
    "pre-commit": "1.1.3",
    "protractor": "4.0.11",
    "reflect-metadata": "0.1.8",
    "require-dir": "0.3.1",
    "rxjs": "5.0.0-beta.12",
    "systemjs-builder": "0.15.34",
    "ts-helpers": "^1.1.1",
    "ts-node": "1.7.0",
    "tslint": "3.15.1",
    "tslint-config-valorsoft": "1.1.4",
    "typedoc": "0.5.1",
    "typescript": "2.0.10",
    "wallaby-webpack": "0.0.26",
    "webdriver-manager": "10.2.8",
    "zone.js": "0.6.26"
  }
}
