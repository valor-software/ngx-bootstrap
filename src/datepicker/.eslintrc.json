{
  "extends": [
    "../../.eslintrc.json"
  ],
  "ignorePatterns": [
    "!**/*"
  ],
  "overrides": [
    {
      "files": [
        "*.ts"
      ],
      "extends": [
        "plugin:@nx/angular",
        "plugin:@angular-eslint/template/process-inline-templates"
      ],
      "parserOptions": {
        "project": [
          "src/datepicker/tsconfig.*?.json"
        ]
      },
      "rules": {
        "@angular-eslint/no-output-on-prefix": 0,
        "@angular-eslint/no-host-metadata-property": 0
      }
    },
    {
      "files": [
        "*.html"
      ],
      "extends": [
        "plugin:@nx/angular-template"
      ],
      "rules": {}
    }
  ]
}
